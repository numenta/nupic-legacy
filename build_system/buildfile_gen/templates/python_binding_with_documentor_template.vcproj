<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioProject
  ProjectType="Visual C++"
  Version="${VisualStudioVersion}"
  Name="nupic_bindings_${DirName}_py"
  ProjectGUID="%(GUID)s"
  RootNamespace="nupic_bindings_${DirName}_py"
  Keyword="Win32Proj"
  >
  <Platforms>
    <Platform
      Name="Win32"
    />
  </Platforms>
  <ToolFiles>
    <ToolFile
      RelativePath="${RelativeRootDir}\build_system\win32\vs-rules\Swig2.rules"
    />
    <ToolFile
      RelativePath="${RelativeRootDir}\build_system\win32\vs-rules\InstallPython.rules"
    />
  </ToolFiles>
  <Configurations>  
    <Configuration
      Name="Debug|Win32"
      OutputDirectory="${Win32InstallDir}/Lib/python${PythonVersion}/site-packages/nupic/bindings"
      IntermediateDirectory="${Win32BuildDir}\lang\python\bindings\nupic_bindings_${DirName}_py\$$(ConfigurationName)"
      ConfigurationType="2"
      CharacterSet="1"
      ManagedExtensions="0"
      WholeProgramOptimization="0"
      >
      <Tool
         Name="VCPreBuildEventTool"
         Description="Prebuild:  creating installation directory"
         CommandLine="python2 ${RelativeRootDir}\build_system\win32\makedir.py &quot;$$(OutDir)&quot;"
      />
      <Tool
        Name="VCCustomBuildTool"
        ExecutionBucket="2"
      />
      <Tool
        Name="Swig"
        ExecutionBucket="3"
      />
      <Tool
        Name="InstallPython"
        ExecutionBucket="4"
      />
      <Tool
        Name="VCCLCompilerTool"
        Optimization="0"
        AdditionalIncludeDirectories="&quot;$$(SolutionDir)&quot;;&quot;$$(SolutionDir)/external/win32/include&quot;;&quot;$$(SolutionDir)/external/common/include&quot;;&quot;$$(SolutionDir)/external/win32/include/python${PythonVersion}&quot;;&quot;$$(SolutionDir)\external\win32\lib\python${PythonVersion}\site-packages\numpy\core\include&quot;"
        PreprocessorDefinitions="NOMINMAX;NTA_INTERNAL;NTA_PYTHON_SUPPORT;NTA_PLATFORM_win32;_CRT_SECURE_NO_DEPRECATE;_SCL_SECURE_NO_WARNINGS;APR_DECLARE_STATIC;APU_DECLARE_STATIC;XML_STATIC;PCRE_STATIC;PCREPOSIX_STATIC;WIN32;_HAS_ITERATOR_DEBUGGING=0;_SECURE_SCL=0;NTA_INSTRUMENT_ON;SWIG_TYPE_TABLE=${SWIGTypeTable};NTA_PYTHON_VERSION_NUM=${PythonVersionNum};NUPIC2"
        RuntimeLibrary="0"
        UsePrecompiledHeader="0"
        EnableEnhancedInstructionSet="2"
        FloatingPointModel="2"
        WarningLevel="3"
        DebugInformationFormat="3"
      />
      <Tool
        Name="VCLinkerTool"
        AdditionalDependencies="apr-1.MT.lib aprutil-1.MT.lib apriconv-1.MT.lib zlib.MT.lib Mswsock.lib ws2_32.lib psapi.lib pcre.MT.lib yamlcpp.MT.lib"
        OutputFile="&quot;$$(OutDir)\_${DirName}.pyd&quot;"
        LinkIncremental="1"
        GenerateManifest="false"
        AdditionalLibraryDirectories="&quot;$$(SolutionDir)/external/win32/lib&quot;;&quot;${Win32PythonDir}\libs&quot;"
        IgnoreDefaultLibraryNames="msvcrt.lib;msvcprt.lib"
        GenerateDebugInformation="true"
        TargetMachine="1"
      />
			<Tool
				Name="VCPostBuildEventTool"
				Description="Run Documentor"
				CommandLine="python2 ..\..\..\..\lang\common\documentor.py &quot;${Win32InstallDir}&quot;"
			/>      
    </Configuration>
    
    <Configuration
      Name="Release|Win32"
      OutputDirectory="${Win32InstallDir}/Lib/python${PythonVersion}/site-packages/nupic/bindings"
      IntermediateDirectory="${Win32BuildDir}\lang\python\bindings\nupic_bindings_${DirName}_py\$$(ConfigurationName)"
      ConfigurationType="2"
      CharacterSet="1"
      ManagedExtensions="0"
      WholeProgramOptimization="1"
      >
      <Tool
         Name="VCPreBuildEventTool"
         Description="Prebuild:  creating installation directory"
         CommandLine="python2 ${RelativeRootDir}\build_system\win32\makedir.py &quot;$$(OutDir)&quot;"
      />
      <Tool
        Name="VCCustomBuildTool"
        ExecutionBucket="2"
      />
      <Tool
        Name="Swig"
        ExecutionBucket="3"
      />
      <Tool
        Name="InstallPython"
        ExecutionBucket="4"
      />
      <Tool
        Name="VCCLCompilerTool"
        Optimization="2"
				EnableIntrinsicFunctions="true"
        AdditionalIncludeDirectories="&quot;$$(SolutionDir)&quot;;&quot;$$(SolutionDir)/external/win32/include&quot;;&quot;$$(SolutionDir)/external/common/include&quot;;&quot;$$(SolutionDir)/external/win32/include/python${PythonVersion}&quot;;&quot;$$(SolutionDir)\external\win32\lib\python${PythonVersion}\site-packages\numpy\core\include&quot;"
        PreprocessorDefinitions="NOMINMAX;NTA_INTERNAL;NTA_PYTHON_SUPPORT;NTA_PLATFORM_win32;_CRT_SECURE_NO_DEPRECATE;_SCL_SECURE_NO_WARNINGS;APR_DECLARE_STATIC;APU_DECLARE_STATIC;XML_STATIC;PCRE_STATIC;PCREPOSIX_STATIC;WIN32;_HAS_ITERATOR_DEBUGGING=0;_SECURE_SCL=0;NTA_INSTRUMENT_ON;SWIG_TYPE_TABLE=${SWIGTypeTable};NTA_PYTHON_VERSION_NUM=${PythonVersionNum};NUPIC2"
        RuntimeLibrary="0"
        UsePrecompiledHeader="0"
        EnableEnhancedInstructionSet="2"
        FloatingPointModel="2"
        WarningLevel="3"
        DebugInformationFormat="3"
      />
      <Tool
        Name="VCLinkerTool"
        AdditionalDependencies="apr-1.MT.lib aprutil-1.MT.lib apriconv-1.MT.lib zlib.MT.lib Mswsock.lib ws2_32.lib psapi.lib pcre.MT.lib yamlcpp.MT.lib"
        OutputFile="&quot;$$(OutDir)\_${DirName}.pyd&quot;"
        LinkIncremental="1"
        GenerateManifest="false"
        AdditionalLibraryDirectories="&quot;$$(SolutionDir)/external/win32/lib&quot;;&quot;${Win32PythonDir}\libs&quot;"
        IgnoreDefaultLibraryNames="msvcrt.lib;msvcprt.lib"
        GenerateDebugInformation="true"
        TargetMachine="1"
      />
			<Tool
				Name="VCPostBuildEventTool"
				Description="Run Documentor"
				CommandLine="python2 ..\..\..\..\lang\common\documentor.py &quot;${Win32InstallDir}&quot;"
			/>      
    </Configuration>
  </Configurations>
  <References>
$References
  </References>
  <Files>
    <Filter
      Name="Swig"
      Filter="i"
      UniqueIdentifier="{00000000-0000-0000-0000-00000000}"
    >
      <File
        RelativePath=".\${DirName}_py.i"
      >
      </File>
    </Filter>
    <Filter
      Name="Source Files"
      Filter="cpp"
      UniqueIdentifier="{00000000-0000-0000-0000-00000000}"
    >
$SourceFiles
    </Filter>
    <Filter
      Name="Header Files"
      Filter="hpp"
      UniqueIdentifier="{00000000-0000-0000-0000-00000000}"
    >
$HeaderFiles
    </Filter>
    <Filter
      Name="Generated Files"
    >
      <File
        RelativePath=".\${DirName}.py"
      >
      </File>
      <File
        RelativePath=".\win_${DirName}_py.h"
      >
      </File>
    </Filter>
  </Files>
  <Globals>
  </Globals>
</VisualStudioProject>
